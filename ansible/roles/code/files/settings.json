{
    "emmet.includeLanguages": {
        "mustache": "html",
        "html-eex": "html"
    },
    "terminal.integrated.fontFamily": "Hack",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 250,
    "files.exclude": {
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.d.ts": {"when": "$(basename).ts"},
        "**/*.js.map": true
    }
}
